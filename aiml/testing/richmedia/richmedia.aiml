     <category>
        <pattern>RCS SHOW LINK BUTTON</pattern>
        <template>
            <button>
                <text>Servusai</text>
                <url>http://www.servusai.com</url>
            </button>
        </template>
    </category>

     <category>
        <pattern>RCS SHOW POSTBACK BUTTON</pattern>
        <template>
            <button>
                <text>Servusai</text>
                <postback>SERVUSAI</postback>
            </button>
        </template>
    </category>

    <category>
        <pattern>RCS SHOW LINK</pattern>
        <template>
            <link>
                <text>Servusai</text>
                <url>http://www.servusai.com</url>
            </link>
        </template>
    </category>

    <category>
        <pattern>RCS SHOW IMAGE</pattern>
        <template>
            <image>
                http://www.servusai.com/servusai.png
            </image>
        </template>
    </category>

    <category>
        <pattern>RCS SHOW VIDEO</pattern>
        <template>
            <video>
                http://www.servusai.com/servusai.mov
            </video>
        </template>
    </category>

    <category>
        <pattern>RCS SHOW CARD</pattern>
        <template>
            <card>
                <image>http://www.servusai.com/servusai.png</image>
                <title>Servusai</title>
                <subtitle>The home of Program Y</subtitle>
                <button>
                    <text>Servusai</text>
                    <url>http://www.servusai.com</url>
                </button>
                <button>
                    <text>AIML</text>
                    <url>http://aiml.foundation</url>
                </button>
            </card>
        </template>
    </category>

    <category>
        <pattern>RCS SHOW CAROUSEL</pattern>
        <template>
            <carousel>
                <card>
                    <image>http://www.servusai.com/servusai.png</image>
                    <title>Servusai</title>
                    <subtitle>The home of Program Y</subtitle>
                    <button>
                        <text>Show me...</text>
                        <url>http://www.servusai.com</url>
                    </button>
                </card>
                <card>
                    <image>http://aiml.foundation/aiml.png</image>
                    <title>AIML Foundation</title>
                    <subtitle>The home of AIML</subtitle>
                    <button>
                        <text>Show me...</text>
                        <url>http://aiml.foundation</url>
                    </button>
                </card>
            </carousel>
        </template>
    </category>

    <category>
        <pattern>RCS SHOW REPLY</pattern>
        <template>
            <reply>
                <text>Servusai</text>
            </reply>
        </template>
    </category>

    <category>
        <pattern>RCS SHOW POSTBACK REPLY</pattern>
        <template>
            <reply>
                <text>Servusai</text>
                <postback>SERVUSAI</postback>
            </reply>
        </template>
    </category>

    <category>
        <pattern>RCS SHOW DELAY</pattern>
        <template>
            <delay>
                <seconds>5</seconds>
            </delay>
        </template>
    </category>

    <category>
        <pattern>RCS SHOW LIST</pattern>
        <template>
            <list>
                <item>Item 1</item>
                <item>Item 2</item>
                <item>Item 3</item>
                <item>Item 4</item>
            </list>
        </template>
    </category>

     <category>
        <pattern>RCS SHOW ORDERED LIST</pattern>
        <template>
            <olist>
                <item>Item 1</item>
                <item>Item 2</item>
                <item>Item 3</item>
                <item>Item 4</item>
            </olist>
        </template>
    </category>

</aiml>