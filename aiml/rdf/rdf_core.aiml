    <category>
        <pattern>GENSYM</pattern>
        <template>
            <condition name="symcount">
                <li value="unknown">symbol
                    <set name="symcount">0</set>
                </li>
                <li>symbol
                    <set name="symcount">
                        <map name="successor">
                            <get name="symcount"/>
                        </map>
                    </set>
                </li>
            </condition>
        </template>
    </category>

    <category>
