    <category>
        <pattern>LARGEROF * AND *</pattern>
        <template>
            <think>
                <set var="thing1">
                    <star/>
                </set>
                <set var="thing2">
                    <star index="2"/>
                </set>
                <set var="result">
                    <srai>IS
                        <get var="thing1"/>
                        LARGER THAN
                        <get var="thing2"/>
                    </srai>
                </set>
            </think>
            <condition var="result">
                <li value="YES">
                    <get var="thing1"/>
                </li>
                <li value="NO">
                    <get var="thing2"/>
                </li>
                <li>unknown</li>
            </condition>
        </template>
    </category>

