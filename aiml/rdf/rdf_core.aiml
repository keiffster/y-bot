                    <think>
                        <set var="head">
                            <srai>LISTCAR
                                <get var="c1"/>
                            </srai>
                        </set>
                        <set var="c1">
                            <srai>LISTCDR
                                <get var="c1"/>
                            </srai>
                        </set>
                        <set var="member">
