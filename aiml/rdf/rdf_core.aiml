    <category>
        <pattern>GETPURPOSE *</pattern>
        <template>
            <think>
                <set var="purpose">
                    <first>
                        <select>
                            <vars>?x</vars>
                            <q>
                                <subj>
                                    <star/>
                                </subj>
                                <pred>hasPurpose</pred>
                                <obj>?x</obj>
                            </q>
                        </select>
                    </first>
                </set>
            </think>
            First tuple =<get var="purpose"/>.
            Get ?x from tuple =
            <get var="?x">
                <tuple>
                    <get var="purpose"/>
                </tuple>
            </get>
            .
            <condition var="purpose">
                <li value="NIL">I don't know what
                    <star/>
                    is for.
                </li>
                <li>
                    <get var="?x">
                        <tuple>
                            <get var="purpose"/>
                        </tuple>
                    </get>
                </li>
            </condition>
        </template>
    </category>

