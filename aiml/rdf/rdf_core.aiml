    <category>
        <pattern>IS * MEMBEROF *</pattern>
        <template>
            <think>
                <set var="types">
                    <srai>WHAT ISA
                        <star/>
                    </srai>
                </set>
                <condition var="types">
                    <li value="NIL">
                        <set var="result">
                            <sraix service="pannous">IS
                                <star/>
                                A
                                <star index="2"/>
                            </sraix>
                        </set>
                    </li>
                    <li>
                        <set var="result">No.</set>
                    </li>
                </condition>
            </think>
            <condition var="types">
                <li value="NIL"/>
                <li>
                    <think>
                        <set var="head">
                            <srai>LISTCAR
                                <get var="types"/>
                            </srai>
                        </set>
                        <set var="types">
                            <srai>LISTCDR
                                <get var="types"/>
                            </srai>
                        </set>
                        <condition>
                            <var>head</var>
                            <value>
                                <star index="2"/>
                            </value>
                            <set var="result">Yes.</set>
                        </condition>
                    </think>
                    <loop/>
                </li>
            </condition>
            <get var="result"/>
        </template>
    </category>

    <category>
        <pattern>ADDTRIPLE * * *</pattern>
        <template>
            <addtriple>
                <subj>
                    <star/>
                </subj>
                <pred>
                    <star index="2"/>
                </pred>
                <obj>
                    <star index="3"/>
                </obj>
            </addtriple>
        </template>
    </category>

    <category>
        <pattern>LIST TRIPLES</pattern>
        <template>
            <select>
                <vars>?subj ?pred ?obj</vars>
                <q>
                    <subj>?subj</subj>
                    <pred>?pred</pred>
                    <obj>?obj</obj>
                </q>
            </select>
        </template>
    </category>

    <category>
        <pattern>FIRST *</pattern>
        <template>
            <first>
                <star/>
            </first>
        </template>
    </category>

    <category>
        <pattern>REST *</pattern>
        <template>
            <rest>
                <star/>
            </rest>
        </template>
    </category>

    <category>
        <pattern>PRINT TRIPLES</pattern>
        <template>
            <think>
                <set var="list">
                    <srai>LIST TRIPLES</srai>
                </set>
            </think>
            <condition var="list">
                <li value="NIL"/>
                <li>
                    <think>
                        <set var="head">
                            <first>
                                <get var="list"/>
                            </first>
                        </set>
                        <set var="list">
                            <rest>
                                <get var="list"/>
                            </rest>
                        </set>
                    </think>
                    ["
                    <get var="?subj">
                        <tuple>
                            <get var="head"/>
                        </tuple>
                    </get>
                    ","
                    <get var="?pred">
                        <tuple>
                            <get var="head"/>
                        </tuple>
                    </get>
                    ","
                    <get var="?obj">
                        <tuple>
                            <get var="head"/>
                        </tuple>
                    </get>
                    "]
                    <loop/>
                </li>
            </condition>
        </template>
    </category>

    <category>
        <pattern>NAME <set>number</set> *</pattern>
        <template>
            <think>
                <set var="number">
                    <star/>
                </set>
                <set var="singular">
                    <map>
                        <name>singular</name>
                        <star index="2"/>
                    </map>
                </set>
                <set var="tuples">
                    <select>
                        <vars>?x</vars>
                        <q>
                            <subj>?x</subj>
                            <pred>isa</pred>
                            <obj>
                                <get var="singular"/>
                            </obj>
                        </q>
                    </select>
                </set>
                <set var="count">
                    <srai>COUNTWORDS
                        <get var="tuples"/>
                    </srai>
                </set>
            </think>
            <condition var="tuples">
                <li value="NIL">
                    <srai>NAME
                        <star/>
                        <star index="2"/>
                    </srai>
                </li>
                <li>I know
                    <get var="count"/>
                    <star index="2"/>.
                    <condition>
                        <li value="NIL" var="tuples"/>
                        <li value="0" var="number"/>
                        <li>
                            <think>
                                <set var="head">
                                    <first>
                                        <get var="tuples"/>
                                    </first>
                                </set>
                                <set var="tuples">
                                    <rest>
                                        <get var="tuples"/>
                                    </rest>
                                </set>
                                <set var="number">
                                    <map>
                                        <name>predecessor</name>
                                        <get var="number"/>
                                    </map>
                                </set>
                            </think>
                            <get var="?x">
                                <tuple>
                                    <get var="head"/>
                                </tuple>
                            </get>
                            <condition>
                                <li value="NIL" var="tuples">.</li>
                                <li value="0" var="number">.</li>
                                <li>,</li>
                            </condition>
                            <loop/>
                        </li>
                    </condition>
                </li>
            </condition>
        </template>
    </category>

</aiml>
