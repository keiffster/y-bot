    <category>
        <pattern>LETTERSINCOMMON * XWORD *</pattern>
        <template>
            <think>
                <set var="any">false</set>
                <set var="letterlist">
                    <explode>
                        <star/>
                    </explode>
                    END
                </set>
            </think>
            <condition var="letterlist">
                <li value="END">
                    <condition value="false" var="any">None</condition>
                </li>
                <li>
                    <think>
                        <set var="first">
                            <srai>FIRSTWORD
                                <get var="letterlist"/>
                            </srai>
                        </set>
                        <set var="letterlist">
                            <srai>REMAININGWORDS
                                <get var="letterlist"/>
                            </srai>
                        </set>
                        <set var="contains">
                            <srai>CONTAINSLETTER
                                <get var="first"/>
                                <star index="2"/>
                            </srai>
                        </set>
                    </think>
                    <condition value="true" var="contains">
                        <think>
                            <set var="any">true</set>
                        </think>
                        <get var="first"/>
                    </condition>
                    <loop/>
                </li>
            </condition>
        </template>
    </category>

