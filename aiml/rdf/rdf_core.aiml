    <category>
        <pattern>ALL ISA</pattern>
        <template>
            <set var="tuples">
                <select>
                    <vars>?x ?y ?z ?w</vars>
                    <q>
                        <subj>?x</subj>
                        <pred>isa</pred>
                        <obj>?y</obj>
                    </q>
                    <q>
                        <subj>?y</subj>
                        <pred>isa</pred>
                        <obj>?z</obj>
                    </q>
                    <q>
                        <subj>?z</subj>
                        <pred>isa</pred>
                        <obj>?w</obj>
                    </q>
                </select>
            </set>
            <condition var="tuples">
                <li value="NIL"/>
                <li>
                    <think>
                        <set var="head">
                            <first>
                                <get var="tuples"/>
                            </first>
                        </set>
                        <set var="tuples">
                            <rest>
                                <get var="tuples"/>
                            </rest>
                        </set>
                    </think>
                    <get var="?x">
                        <tuple>
                            <get var="head"/>
                        </tuple>
                    </get>
                    --&gt;
                    <get var="?y">
                        <tuple>
                            <get var="head"/>
                        </tuple>
                    </get>
                    --&gt;
                    <get var="?z">
                        <tuple>
                            <get var="head"/>
                        </tuple>
                    </get>
                    --&gt;
                    <get var="?w">
                        <tuple>
                            <get var="head"/>
                        </tuple>
                    </get>
                    ,
                    <loop/>
                </li>
            </condition>
        </template>
    </category>

