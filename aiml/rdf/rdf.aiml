                                </pred>
                                <obj>
                                    <get var="thing2"/>
                                </obj>
                            </q>
                        </select>
                    </srai>
                </set>
                <set var="case2">
                    <srai>APPEND
                        <select>
                            <q>
                                <subj>
                                    <get var="thing2"/>
                                </subj>
                                <pred>
                                    <get var="pred"/>
                                </pred>
                                <obj>
                                    <get var="thing1"/>
                                </obj>
                            </q>
                        </select>

                        <select>
                            <vars>?x</vars>
                            <q>
                                <subj>
                                    <get var="thing2"/>
                                </subj>
                                <pred>
                                    <get var="pred"/>
                                </pred>
                                <obj>?x</obj>
                            </q>
                            <q>
                                <subj>?x</subj>
                                <pred>
                                    <get var="pred"/>
                                </pred>
                                <obj>
                                    <get var="thing1"/>
                                </obj>
                            </q>
                        </select>
                    </srai>
                </set>
            </think>
            <condition var="thing1">
                <li>
                    <value>
                        <get var="thing2"/>
                    </value>
                    same
                </li>
                <li>
                    <condition var="case1">
                        <li value="NIL">
                            <condition var="case2">
                                <li value="NIL">unknown</li>
                                <li>No</li>
                            </condition>
                        </li>
                        <li>
                            <condition var="case2">
                                <li value="NIL">Yes</li>
                                <li>unknown</li>
                            </condition>
                        </li>
                    </condition>
                </li>
