    <category>
        <pattern>I HAVE A <set>color</set> *
        </pattern>
        <template>
            <think>
                <set var="color">
                    <star/>
                </set>
                <set var="noun">
                    <star index="2"/>
                </set>
                <set var="symbol">
                    <srai>GENSYM</srai>
                </set>
                <set var="obj">
                    <star index="2"/>
                </set>
                <set var="description">a
                    <star/>
                    <star index="2"/>
                </set>
                <addtriple>
                    <subj>
                        <id/>
                    </subj>
                    <pred>has</pred>
                    <obj>
                        <get var="symbol"/>
                    </obj>
                </addtriple>
                <addtriple>
                    <subj>
                        <get var="symbol"/>
                    </subj>
                    <pred>hasColor</pred>
                    <obj>
                        <get var="color"/>
                    </obj>
                </addtriple>
                <addtriple>
                    <subj>
                        <get var="symbol"/>
                    </subj>
                    <pred>hasDescription</pred>
                    <obj>
                        <get var="description"/>
                    </obj>
                </addtriple>
                <addtriple>
                    <subj>
                        <get var="symbol"/>
                    </subj>
                    <pred>isA</pred>
                    <obj>
                        <get var="obj"/>
                    </obj>
                </addtriple>
            </think>
            <star/>
            is a nice color of<star index="2"/>.
        </template>
    </category>

    <category>
        <pattern>WHAT DO I HAVE ^</pattern>
        <template>
            <set var="tuples">
                <select>
                    <vars>?sym</vars>
                    <q>
                        <subj>
                            <id/>
                        </subj>
                        <pred>has</pred>
                        <obj>?sym</obj>
                    </q>
                </select>
            </set>
            <condition var="tuples">
                <li value="NIL">I don't know. What do you have?</li>
                <li>
                    <condition var="tuples">
                        <li value="NIL"/>
                        <li>
                            <think>
                                <set var="head">
                                    <first>
                                        <get var="tuples"/>
                                    </first>
                                </set>
                                <set var="tuples">
                                    <rest>
                                        <get var="tuples"/>
                                    </rest>
                                </set>
                            </think>
                            <uniq>
                                <subj>
                                    <get var="?sym">
                                        <tuple>
                                            <get var="head"/>
                                        </tuple>
                                    </get>
                                </subj>
                                <pred>hasDescription</pred>
                                <obj>?description</obj>
                            </uniq>
                            <condition var="tuples">
                                <li value="NIL"/>
                                <li>,</li>
                            </condition>
                            <loop/>
                        </li>
                    </condition>
                </li>
            </condition>
        </template>
    </category>

    <category>
