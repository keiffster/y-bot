    <category>
        <pattern>SINGULAR
            <set>animals</set>
        </pattern>
        <template>
            <uniq>
                <subj>?singular</subj>
                <pred>hasPlural</pred>
                <obj>
                    <star/>
                </obj>
            </uniq>
        </template>
    </category>
    <category>
        <pattern>PLURAL
            <set>animal</set>
        </pattern>
        <template>
            <uniq>
                <subj>
                    <star/>
                </subj>
                <pred>hasPlural</pred>
                <obj>?plural</obj>
            </uniq>
        </template>
    </category>
    <category>
        <pattern>
            <set>animals</set>
        </pattern>
        <template>
            <star/>
            is the plural of
            <uniq>
                <subj>?subj</subj>
                <pred>hasPlural</pred>
                <obj>
                    <star/>
                </obj>
            </uniq>
            .
        </template>
    </category>
