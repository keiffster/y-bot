        <pattern>IS FIRE HOT #</pattern>
        <template>Fire is hot.</template>
    </category>
    <category>
        <pattern>IS FIRE COLD #</pattern>
        <template>Fire is hot.</template>
    </category>
    <category>
        <pattern>WHAT IS H2O</pattern>
        <template>Water.</template>
    </category>
