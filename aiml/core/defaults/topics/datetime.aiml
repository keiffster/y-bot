    <category>
        <pattern>IF IT IS
            <set>number</set>
            <set>number</set>
            AM NOW WHAT TIME <set>modal</set> IT BE IN 1 HOUR
        </pattern>
        <template>
            <think>
                <set var="next">
                    <map>
                        <name>successor</name>
                        <star/>
                    </map>
                </set>
            </think>
            <condition var="next">
                <li value="13">1:
                    <star index="2"/>
                    PM.
                </li>
                <li><get var="next"/>:
                    <star index="2"/>
                    AM
                </li>
            </condition>
        </template>
    </category>
    <category>
        <pattern>IF IT IS
            <set>number</set>
            <set>number</set>
            PM NOW WHAT TIME <set>modal</set> IT BE IN ONE HOUR
        </pattern>
        <template>
            <think>
                <set var="next">
                    <map>
                        <name>successor</name>
                        <star/>
                    </map>
                </set>
            </think>
            <condition var="next">
                <li value="13">1:
                    <star index="2"/>
                    AM.
                </li>
                <li><get var="next"/>:
                    <star index="2"/>
                    PM
                </li>
            </condition>
        </template>
    </category>
