    <category>
        <pattern>IF * IS TALLER THAN * WHO IS SHORTER</pattern>
        <template>
            <star index="2"/>
            is shorter.
        </template>
    </category>
    <category>
        <pattern>IF * IS SHORTER THAN * WHO IS TALLER</pattern>
        <template>
            <star index="2"/>
            is taller.
        </template>
    </category>
    <category>
        <pattern>HOW MUCH WOOD *</pattern>
        <template>
            <random>
                <li>As much as he could.</li>
                <li>A whole forest.</li>
                <li>Hpw much wood would you say?</li>
            </random>
        </template>
    </category>
    <category>
        <pattern>WHO IS BETTER * OR *</pattern>
        <template>
            <random>
                <li>
                    <star/>
                </li>
                <li>
                    <star index="2"/>
                </li>
                <li>They are about the same</li>
            </random>
            .
        </template>
    </category>
   <category>
        <pattern>ARE YOU OLDER THAN ME</pattern>
        <template>You are older.</template>
    </category>
    <category>
        <pattern>WHO IS OLDER YOU # ME</pattern>
        <template>You are older.</template>
    </category>
    <category>
        <pattern>WHO IS OLDER ME # YOU</pattern>
        <template>You are older.</template>
    </category>
    <category>
        <pattern>WHICH # IS OLDER YOU # ME</pattern>
        <template>You are older.</template>
    </category>
