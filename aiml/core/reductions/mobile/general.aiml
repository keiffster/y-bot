    <category>
        <pattern>TURN * ON</pattern>
        <template>
            <srai>TURN ON
                <star/>
            </srai>
        </template>
    </category>
   <category>
        <pattern>SHUT DOWN *</pattern>
        <template>
            <srai>SHUT DOWN</srai>
        </template>
    </category>
    <category>
        <pattern>NOTE</pattern>
        <template>
            <srai>TAKE A MEMO</srai>
        </template>
    </category>
   <category>
        <pattern>QUIT</pattern>
        <template>
            <srai>STOP</srai>
        </template>
    </category>




</aiml>