    </category>
    <category>
        <pattern>HOW DO I GET TO *</pattern>
        <template>
            <srai>DIRECTIONS TO
                <star/>
            </srai>
        </template>
    </category>
    <category>
        <pattern>HOW DO I GET HOME</pattern>
        <template>
            <condition name="residence">
                <li value="where">Where is your residence?</li>
                <li>
                    <srai>DIRECTIONS TO
                        <get name="residence"/>
                    </srai>
                </li>
            </condition>
        </template>
    </category>
