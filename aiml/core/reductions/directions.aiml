    <category>
        <pattern>HOW FAR IS TO FROM * TO *</pattern>
        <template>
            <srai>DIRECTIONS FROM
                <star/>
                TO
                <star index="2"/>
            </srai>
        </template>
    </category>
   <category>
        <pattern>DRIVING DISTANCE *</pattern>
        <template>
            <srai>DIRECTIONS
                <star/>
            </srai>
        </template>
    </category>
    <category>
        <pattern>GET ME DIRECTIONS *</pattern>
        <template>
            <srai>DIRECTIONS
                <star/>
            </srai>
        </template>
    </category>
    <category>
        <pattern>HOW DO YOU GET TO *</pattern>
        <template>
            <srai>DIRECTIONS TO
                <star/>
            </srai>
        </template>
    </category>
    <category>
        <pattern>HOW FAR IS * FROM *</pattern>
        <template>
            <srai>DIRECTIONS FROM
                <star/>
                TO
                <star/>
            </srai>
        </template>
    </category>
    <category>
        <pattern>GIVE ME DIRECTIONS TO *</pattern>
        <template>
