    <category>
        <pattern>
           NEWS * MAX * SORT * REVERSE *
        </pattern>
        <template>
            <think>
                <set name="active"><bot name="newsapi" /></set>
            </think>
            <condition name="active">
                <li value="true">
                    <extension path="programy.extensions.newsapi.newsapi.NewsAPIExtension">
                        SOURCE <star index="1"/> MAX <star index="2"/> SORT <star index="3"/> REVERSE <star index="4"/>
                    </extension>
                </li>
                <li>Unfortunately my News Feed is not listening right now, you will need to speak to my bot master to enable it.</li>
            </condition>
        </template>
    </category>

    <category>
        <pattern>ABC NEWS</pattern>
