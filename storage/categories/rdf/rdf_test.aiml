                <obj>DOG</obj>
            </addtriple>
            <addtriple>
                <subj>ROVER</subj>
                <pred>ISA</pred>
                <obj>DOG</obj>
            </addtriple>
            <addtriple>
                <subj>SNOOPY</subj>
                <pred>ISA</pred>
                <obj>DOG</obj>
            </addtriple>
            Select:
            <select>
                <vars>?x ?y</vars>
                <q>
                    <subj>?x</subj>
                    <pred>?y</pred>
                    <obj>DOG</obj>
                </q>
            </select>
            Deleting triples:
            <deletetriple>
                <subj>FIDO</subj>
                <pred>ISA</pred>
                <obj>DOG</obj>
            </deletetriple>
            <deletetriple>
                <subj>ROVER</subj>
                <pred>ISA</pred>
                <obj>DOG</obj>
            </deletetriple>
            <deletetriple>
                <subj>REX</subj>
                <pred>ISA</pred>
                <obj>DOG</obj>
            </deletetriple>
            <deletetriple>
                <subj>FIDO</subj>
                <obj>DOG</obj>
            </deletetriple>
            Select:
            <select>
                <vars>?x ?y</vars>
                <q>
                    <subj>?x</subj>
                    <pred>?y</pred>
                    <obj>DOG</obj>
                </q>
            </select>
        </template>
    </category>

    <category>
        <pattern>SELECT TEST</pattern>
        <template>
            <set var="tuples">
                <select>
                    <vars>?x</vars>
                    <q>
                        <subj>?x</subj>
                        <pred>hasSize</pred>
                        <obj>7</obj>
                    </q>
                    <q>
                        <subj>?x</subj>
                        <pred>lifeArea</pred>
                        <obj>Physical</obj>
                    </q>
                </select>
            </set>
            <condition var="tuples">
                <li value="NIL"/>
                <li>
                    <think>
                        <set var="head">
                            <first>
                                <get var="tuples"/>
                            </first>
                        </set>
                        <set var="tuples">
                            <rest>
                                <get var="tuples"/>
                            </rest>
                        </set>
                    </think>
                    <get var="?x">
                        <tuple>
                            <get var="head"/>
                        </tuple>
                    </get>
                    <loop/>
                </li>
            </condition>
        </template>
    </category>

