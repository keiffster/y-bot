    <category>
		<pattern>TRIPLE TEST * *</pattern>
		<template>
			<select>
				<vars>?x</vars>
				<q><subj><star index="1"/></subj><pred><star index="2"/></pred><obj>?x</obj></q>
			</select>
		</template>
	</category>

    <category>
        <pattern>UNIQ TEST</pattern>
        <template>
            <uniq>
                <subj>York</subj>
                <pred>isa</pred>
                <obj>?object</obj>
            </uniq>
        </template>
    </category>

    <category>
        <pattern>UNIQ TEST 2</pattern>
        <template>
            <addtriple>
                <subj>10099</subj>
                <pred>hasFirstName</pred>
                <obj>Alan</obj>
            </addtriple>
            <addtriple>
                <subj>10099</subj>
                <pred>hasLastName</pred>
                <obj>Turing</obj>
            </addtriple>
            <addtriple>
                <subj>10099</subj>
                <pred>hasMobileNumber</pred>
                <obj>2125551212</obj>
            </addtriple>
            <set var="id">
                <uniq>
                    <subj>?id</subj>
                    <pred>hasFirstName</pred>
                    <obj>Alan</obj>
                </uniq>
            </set>
            <uniq>
                <subj>
                    <get var="id"/>
                </subj>
                <pred>hasMobileNumber</pred>
                <obj>?number</obj>
            </uniq>
        </template>
    </category>

    <category>
        <pattern>UNIQ TEST 3</pattern>
        <template>
            <addtriple>
                <subj>X</subj>
                <pred>Y</pred>
                <obj>Z</obj>
            </addtriple>
            <set var="uniqvar">?x</set>
            <uniq>
                <subj>
                    <get var="uniqvar"/>
                </subj>
                <pred>Y</pred>
                <obj>Z</obj>
            </uniq>
        </template>
    </category>

