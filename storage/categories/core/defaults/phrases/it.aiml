        <template>
            <random>
                <li>What is it?</li>
                <li>Do you know what it is?</li>
                <li>Something, or nothing?</li>
            </random>
            <think>
                <set name="it">
                    <set name="topic">
                        <person/>
                    </set>
                </set>
            </think>
        </template>
